{
    "extends": "solium:recommended",
    "plugins": [
        "security"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "indentation": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}
